.index {
	border-collapse:collapse;
	th {
		border: 1px black;
		border-bottom-style: solid;
		padding: 5px;
	}
	td {
		padding: 5px;
	}
}

.leaderboard {
	display: inline-block;
	padding: 00px 30px 0px 30px;
}

tr {
	border-collapse:collapse;
}

tr:nth-child(even) {
	background-color:rgb(200,230,255);
}

tr:nth-child(odd) {
	background-color:rgb(255,255,255);
}